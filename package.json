{
  "name": "443-elevator",
  "version": "2.1.0",
  "private": true,
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && npx tailwindcss -i ./src/main.css -o ./dist/output.css && webpack --config webpack.prod.js",
    "build:analyze": "ANALYZE=true npm run build",
    "dev": "webpack serve --config webpack.dev.js --open",
    "preview": "npm run build && npx serve dist",
    "start": "echo 'This site is intended to be served as static files. Run via `npm run build` instead.' && exit 1"
  },
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.7",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.24.7",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^9.1.0",
    "concurrently": "^7.6.0",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^6.7.3",
    "eslint": "^8.30.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-standard-with-typescript": "^24.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-n": "^15.6.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-tailwindcss": "^3.17.4",
    "html-webpack-plugin": "^5.6.0",
    "postcss": "^8.4.20",
    "postcss-loader": "^8.1.1",
    "rimraf": "^6.0.1",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.1",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
