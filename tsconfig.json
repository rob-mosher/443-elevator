{
  "compilerOptions": {
    "target": "es2016",
    "jsx": "react",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": [
    "src/**/*",
    "tailwind.config.ts",
    "webpack.config.ts",
    "webpack.dev.ts",
    "webpack.prod.ts"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}